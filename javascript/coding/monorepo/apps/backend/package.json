  },
  "dependencies": {
    "@google/genai": "^1.6.0",
    "@octokit/rest": "^22.0.0",
    "@tester/types": "*",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "rsdi": "^3.0.4"
  }
